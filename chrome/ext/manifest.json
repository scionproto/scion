  "name": "SCION Link Feeder Test",
  "version": "0.1",
  "description": "Request link data and manage proxy to the SCION network.",
  "manifest_version": 2,
  "default_locale": "en",
  "icons": {
    "16": "img/icon16.png",
    "32": "img/icon32.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "key": "",
  "browser_action": {
    "default_icon": "img/icon16.png",
    "default_popup": "popup.html",
    "default_title": "SCION Proxy Settings"
  },
  "background": {
    "scripts": [
      "proxy_form_controller.js",
      "proxy_error_handler.js",
      "background.js"
      //"contextmenu.js"
    ]
  },
  "permissions": [
    "tabs",
    "<all_urls>",
    "webRequest",
    "proxy"
    //"contextMenus"
  ]
}
