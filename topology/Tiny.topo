--- # Tiny Topology
defaults:
  zookeepers:
    1:
      manage: false
      addr: 127.0.0.1
ASes:
  1-11:
    core: true
    mtu: 1400
  1-12:
    cert_issuer: 1-11
  1-13:
    cert_issuer: 1-11
CAs:
  CA1-1:
    ISD-AS: 1-11
    commonName: CA1-1
    trc_servers: 1
    arpki_servers: 1
LogServers:
  Log1:
    ISD-AS: 1-11
RAINS:
  1:
    trc_servers: {1-11: 1}
links:
  - {a: 1-11, b: 1-12, ltype: PARENT, mtu: 1280}
  - {a: 1-11, b: 1-13, ltype: PARENT, bw: 500}
