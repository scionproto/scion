export GOPATH="$HOME/go"
export PATH="$HOME/.local/bin:/usr/share/zookeeper/bin:$GOPATH/bin:/usr/local/go/bin:$PATH"
export SHELL
export BASEDIR="$GOPATH/src/github.com/scionproto/scion"
export PYTHONPATH="$BASEDIR/python/:$BASEDIR"
export USER=scion
export LOGNAME=scion

alias cdscion="cd $BASEDIR"
