  - {a: 2-1, b: 3-1, ltype: CORE}
  - {a: 2-1, b: 2-3, ltype: PARENT}
  - {a: 2-1, b: 2-4, ltype: PARENT}
  - {a: 2-2, b: 2-3, ltype: PARENT}
  - {a: 2-2, b: 2-4, ltype: PARENT}
  - {a: 2-3, b: 2-4, ltype: PEER}
